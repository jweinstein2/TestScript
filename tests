echo aaabbb | $PROG aa a -   abb aab -
echo abbb | $PROG aa a -F2 abb aab -S1
echo abbb | $PROG aa a -S2 abb aab -S0
echo aabbbc | $PROG bd a -S2 aa a - abb aab -S1 c d -S
